{
  "testRegex": "/__tests__/.*\\.test\\.js$",
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js"
  ],
  "coverageDirectory": "<rootDir>/coverage/",
  "coverageReporters": [
    "lcov"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "rootDir": "./../",
  "automock": false,
  "verbose": false
}